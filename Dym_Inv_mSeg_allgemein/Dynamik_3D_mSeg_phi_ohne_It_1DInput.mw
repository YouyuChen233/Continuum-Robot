<Group labelreference="L361" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i to ndsk do 
    if irem(i, 2) = 1 then 
        for ii to nseil do 
            d_ss[ii][i] := sqrt(2*(1/kappa[i] - r_h_wert*cos(varphi[i] - psi[ii]) + DR[ii]*tan(0.5*vartheta[i]))^2*(1 - cos(vartheta[i]))) 
                          - 2*DR[ii]/cos(0.5*vartheta[i]): 
            delta_l[ii][i] := d_ss[ii][i] + (-L0 + 2*DR[ii]): 
        end do: 
    else 
        for ii to nseil do 
            d_ss[ii][i] := sqrt(2*(1/kappa[i] - r_h_wert*cos(varphi[i] - psi[ii]) + DL[ii]*tan(0.5*vartheta[i]))^2*(1 - cos(vartheta[i]))) 
                          - 2*DL[ii]/cos(0.5*vartheta[i]): 
            delta_l[ii][i] := d_ss[ii][i] + (-L0 + 2*DL[ii]): 
        end do: 
    end if: 
end do:
for i to nseil do
    dd_ss[i] := diff(d_ss[i], t):
end do:
for i from ndsk - 1 by -1 to 1 do
    for ii to nseil do 
        dd_dsk[ii][i] := dd_dsk[ii][i + 1] + dd_ss[ii][i + 1]: 
    end do:
end do:
for i to ndsk - 1 do
    eta[i] := arccos(DotProduct(convert(P_h[1 .. -1, i], list), convert(P_h[1 .. -1, i + 1], list))):
end do:</Text-field>
</Input>
</Group>
