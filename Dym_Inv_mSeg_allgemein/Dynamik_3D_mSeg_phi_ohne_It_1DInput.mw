<Group labelreference="L359" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">for i to nseil do 
    d_ss[i] := Vector(ndsk)^%T: 
    dd_ss[i] := Vector(ndsk)^%T: 
    dd_dsk[i] := Vector(ndsk)^%T: 
    F_reib[i] := Vector(ndsk)^%T: 
    delta_l[i] := Vector(ndsk)^%T: 
end do:
eta := Vector(ndsk)^%T:</Text-field>
</Input>
